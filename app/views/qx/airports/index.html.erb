<p id="notice"><%= notice %></p>

<h1>Qx Airports</h1>

<table>
  <tbody>
      <tr>
        <%= search_form_for @q do |f| %>
          <td>
            <%= f.label :city_ch_ame %>
            <%= f.search_field :city_ch_ame_cont %>
          </td>
          <td>
            <%= f.label :country_name %>
            <%= f.search_field :country_name_cont %>
          </td>
          <td>
            <%= f.label :airport_4_code %>
            <%= f.search_field :airport_4_code_eq %>
          </td>

        <td>
          <%= f.label :"检索" %>
          <%= f.submit %>
        </td>
        <% end %>
      </tr>
  </tbody>
</table>


<table>
  <thead>
    <tr>
      <th>City name</th>
      <th>Airport 3 code</th>
      <th>City ch ame</th>
      <th>Country name</th>
      <th>Airport 4 code</th>
      <th>Airport name</th>
      <th>English name</th>
      <th>Longgitude</th>
      <th>Latitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qx_airports.each do |qx_airport| %>
      <tr>
        <td><%= qx_airport.city_name %></td>
        <td><%= qx_airport.airport_3_code %></td>
        <td><%= qx_airport.city_ch_ame %></td>
        <td><%= qx_airport.country_name %></td>
        <td><%= qx_airport.airport_4_code %></td>
        <td><%= qx_airport.airport_name %></td>
        <td><%= qx_airport.english_name %></td>
        <td><%= qx_airport.longgitude %></td>
        <td><%= qx_airport.latitude %></td>
        <td><%= link_to 'Show', qx_airport %></td>
        <td><%= link_to 'Edit', edit_qx_airport_path(qx_airport) %></td>
        <td><%= link_to 'Destroy', qx_airport, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qx Airport', new_qx_airport_path %>

<%= will_paginate @qx_airports %>