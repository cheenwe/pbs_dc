<p id="notice"><%= notice %></p>

<h1>Dc Photos</h1>

<%= search_form_for @q do |f| %>

  <%= f.label :user_id %>
  <%= f.search_field :user_id_cont %>

  <%= f.submit %>
<% end %>

<br>
<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dc_photos.each do |dc_photo| %>
      <tr>
        <td><%= dc_photo.user_id %></td>
        <td> <%= image_tag(dc_photo.url) rescue '' %></td>
        <td><%= link_to 'Show', dc_photo %></td>
        <td><%= link_to 'Edit', edit_dc_photo_path(dc_photo) %></td>
        <td><%= link_to 'Destroy', dc_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= will_paginate @dc_photos %>

<br>

<%= link_to 'New Dc Photo', new_dc_photo_path %>
