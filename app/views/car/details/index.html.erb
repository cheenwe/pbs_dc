<p id="notice"><%= notice %></p>

<h1>Car Details<small> &nbsp;<%= @data.size %></small></h1>
<form class="form-inline" action="details" accept-charset="UTF-8" method="get" style="margin-bottom:20px">
  <input type="search" class="mr-3 form-control me-font-2" style="height:35px" name="q[item_id_eq]" placeholder="输入Item id" value="" autocomplete="off">
<button type="submit" class="btn btn-primary me-font-1">搜 索</button>
</form>


<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Config_id</th>
      <th>Key</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_details.each do |car_detail| %>
      <tr>
        <td><%= car_detail.item_id %></td>
        <td><%= car_detail.config_id %></td>
        <td><%= car_detail.key %></td>
        <td><%= car_detail.value %></td>
        <td><%= link_to 'Show', car_detail %></td>
        <td><%= link_to 'Edit', edit_car_detail_path(car_detail) %></td>
        <td><%= link_to 'Destroy', car_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Detail', new_car_detail_path %>

<%= will_paginate @car_details %>
