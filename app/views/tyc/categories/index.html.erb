<p id="notice"><%= notice %></p>

<h1>Tyc Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tyc_categories.each do |tyc_category| %>
      <tr>
        <td><%= tyc_category.name %></td>
        <td><%= link_to 'Show', tyc_category %></td>
        <td><%= link_to 'Edit', edit_tyc_category_path(tyc_category) %></td>
        <td><%= link_to 'Destroy', tyc_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tyc Category', new_tyc_category_path %>
